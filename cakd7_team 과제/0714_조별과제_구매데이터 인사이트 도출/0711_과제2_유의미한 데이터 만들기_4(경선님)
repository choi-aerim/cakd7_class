    "#배열-> 시리즈\n",
    "cusno=pd.Series(cusno)\n",
    "gender=pd.Series(gender)\n",
    "age=pd.Series(age)\n",
    "region=pd.Series(region)\n",
    "\n",
    "#데이터프레임 만들기\n",
    "customer=pd.concat([cusno,gender,age,region],axis=1)\n",
    "\n",
    "#컬럼명 지정\n",
    "customer.columns=['cusno','gender','age','region']\n",
    "\n",
    "customer.head()"
   ]

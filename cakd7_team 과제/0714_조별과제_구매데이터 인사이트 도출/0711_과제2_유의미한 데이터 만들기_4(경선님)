    "                     columns=['year','detail_product'],\n",
    "                     values='amount',\n",
    "                     aggfunc=['count','sum']\n",
    "                    )\n",
    "\n",
    "table5=table3[('count',2020)].append(table3[('count',2021)])\n",
    "table5=table5.append(table5.iloc[1]-table5.iloc[0]).T\n",
    "\n",
    "table5.columns=['2020년도','2021년도','구매 수량 차이(21년도-20년도)']\n",

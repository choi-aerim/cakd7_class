   "source": [
    "#age 범주화\n",
    "\n",
    "def age_category(x):\n",
    "    y=0\n",
    "    if x<20:\n",
    "        y='10대'\n",
    "    elif x<30:\n",
    "        y='20대'\n",
    "    elif x<40:\n",
    "        y='30대'\n",
    "    elif x<50:\n",
    "        y='40대'\n",
    "    elif x<60:\n",
    "        y='50대'\n",
    "    else:\n",
    "        y='60대'\n",
    "    return y\n",
    "\n",
    "df['age_c']=df['age'].apply(age_category)\n",
    "df.head()"
   ]
  },
